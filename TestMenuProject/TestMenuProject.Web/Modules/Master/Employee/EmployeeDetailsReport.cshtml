
@model TestMenuProject.Master.EmployeeDetailsReportData

@if ((bool?)ViewData["Printing"] == true)
{
    Layout = MVC.Views.Shared._LayoutNoNavigation;
    ViewData["Title"] = " Employee Details";
}
else
{
    ViewData["Title"] = " Employee Details";
}

<section class="invoice">
    <div class="row">
        <div class="col-xs-12">
            <h2 class="page-header">
                Employee Details
                <small class="pull-right">Date: @DateTime.Now.ToShortDateString()</small>
            </h2>
        </div>
    </div>
    <!-- Table row -->
    <div class="row">
        <div class="col-xs-12 table-responsive">
			<table class="table table-striped">
				<thead>
					<tr>
						<th style="text-align:left">Employee Name</th>
						<th style="text-align:right">Country</th>
					</tr>
				</thead>
				<tbody>
					<tr>
						<td style="text-align:left">@Model.employee.EmployeeName</td>
						<td style="text-align: left">@Model.employee.CountryCountryName</td>
					</tr>

				</tbody>
			</table>
        </div>
        <!-- /.col -->
    </div>
</section>